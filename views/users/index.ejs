<!DOCTYPE html>
<html lang='en'>

<head>
    <%- include ('../partials/head.ejs') %>
</head>

<body>

    <table class="table">
        <thead>
            <tr>
                <th scope='col'>#</th>
                <th scope='col'>Name</th>
                <th scope='col'>Department</th>
                <th scope='col'>Position</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach((users, index) => { %>
            <tr>
                <th scope='row'><%= index + 1 %></th>
                <td><%= users.name %></td>
                <td><%= users.department %></td>
                <td><%= users.position %></td>
            </tr>
            <%}) %>
        </tbody>
    </table>
</body>

</html>